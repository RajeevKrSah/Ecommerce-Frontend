{
  "name": "secureauth-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "SecureAuth Frontend - Enterprise Authentication Platform",
  "author": "SecureAuth Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
